<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@color/authui_inputTextColorDark"
    tools:context="com.domily.android.domily.MainActivity">

    <Button
        android:id="@+id/ButtonBuscarTienda"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Buscar Tienda"
        android:layout_marginTop="216dp"
        android:layout_alignParentTop="true"
        android:layout_centerHorizontal="true"/>

    <Button
        android:id="@+id/ButtonRegistrarTienda"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Registrar Tienda"
        android:layout_alignParentBottom="true"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true"
        android:layout_marginLeft="34dp"
        android:layout_marginStart="34dp"
        android:layout_marginBottom="25dp"/>




</RelativeLayout>
